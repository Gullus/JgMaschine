<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <bindings>
      <netMsmqBinding>
        <binding name="msmqConfiguration">
          <security mode="None" />
        </binding>  
      </netMsmqBinding>
    </bindings>
    <behaviors />
    <services>
      <service behaviorConfiguration="" name="JgMaschineTest.MsmqIn">
        <endpoint binding="netMsmqBinding" bindingConfiguration="msmqConfiguration"
          name="msmqEndpoint" contract="JgMaschineTest.IMsmqDaten" />
        <host>
          <baseAddresses>
            <add baseAddress="net.msmq://localhost/private/OutSchlange" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <connectionStrings>
    <add name="JgModelContainer" connectionString="metadata=res://*/JgModel.csdl|res://*/JgModel.ssdl|res://*/JgModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.\SqlExpress;initial catalog=JgMaschine;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
    <add name="JgMaschineTest.Properties.Settings.Verbindungszeichen" connectionString="Data Source=.\SqlExpress;Initial Catalog=JgMaschine;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="JgMaschineTest.Properties.Settings.VerbindungClient" connectionString="Data Source=.\SqlExpress;Initial Catalog=JgMaschine;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="JgMaschineTest.Properties.Settings.VerbindungServer" connectionString="Data Source=.\SqlExpress;Initial Catalog=JgMaschineServer;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  
  <applicationSettings>
    <JgMaschineTest.Properties.Settings>
      <setting name="ScannerAdresse" serializeAs="String">
        <value>192.168.1.100</value>
      </setting>
      <setting name="ScannerPort" serializeAs="String">
        <value>51000</value>
      </setting>
    </JgMaschineTest.Properties.Settings>
  </applicationSettings>
  <userSettings>
    <JgMaschineTest.Properties.Settings>
      <setting name="MaschinenAdresse" serializeAs="String">
        <value>gullus-server</value>
      </setting>
      <setting name="MaschinenPfad" serializeAs="String">
        <value>c\Progress\Pro2\impdata</value>
      </setting>
      <setting name="BenutzerName" serializeAs="String">
        <value>jg</value>
      </setting>
      <setting name="BenutzerKennwort" serializeAs="String">
        <value>pezdx65</value>
      </setting>
      <setting name="FastCubeConnection" serializeAs="String">
        <value>Provider=SQLNCLI11.1;Integrated Security=SSPI;Persist Security Info=False;Initial Catalog=JgMaschine;Data Source=.\SqlExpress
</value>
      </setting>
    </JgMaschineTest.Properties.Settings>
  </userSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
